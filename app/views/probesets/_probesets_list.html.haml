%br
- if @probesets.size == 0
  %p
    No probesets found...
- else
  %p
    = found_count("probesets", @probesets)
  %p
    = render(:partial => "shared/pagination", :locals => {:items => @probesets})
  %br
  %table{:width => "100%", :border => "0", :cellpadding => "0", :cellspacing => "2"}
    %thead
      %tr.row-header
        %td{:width => "30%"}
          Name
        %td{:width => "15%"}
          RGD Gene ID
        %td{:width => "15%"}
          Gene symbol
        %td{:width => "40%"}
          Platform
    %tbody#staffTable
      = render(:partial => "probeset.html.haml", :collection => @probesets)
