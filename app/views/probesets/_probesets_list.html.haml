%br
- if @probesets.size == 0
  %p
    No probesets found...
- else
  %p
    = found_count("probesets", @probesets)
  %p
    = render(:partial => "shared/pagination", :locals => {:items => @probesets})
  %br
  %table{:width => "100%", :border => "0", :cellpadding => "0", :cellspacing => "2"}
    %thead
      %tr.rowHeader
        %td{:width => "50%"}
          %b
            Name
        %td{:width => "50%"}
          %b
            Platform
    %tbody#staffTable
      = render(:partial => "probeset", :collection => @probesets)
