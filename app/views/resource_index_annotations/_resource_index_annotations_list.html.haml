%br
- if @resource_index_annotations.size == 0
  %p
    No resource index annotations found...
- else
  %p
    = found_count("resource index annotations", @resource_index_annotations)
  %p
    = render(:partial => "shared/pagination.html.haml", :locals => {:items => @resource_index_annotations})
  %br
  %table{:width => "100%", :border => "0", :cellpadding => "0", :cellspacing => "2"}
    %thead
      %tr.row-header
        %td{:width => "70%"}
          GEO accession
    %tbody#staffTable
      = render(:partial => "resource_index_annotation.html.haml", :collection => @resource_index_annotations)
